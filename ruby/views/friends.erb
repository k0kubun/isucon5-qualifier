<h2>友だちリスト</h2>
<div class="row panel panel-primary" id="friends">
  <dl>
    <% eager_load_get_user(friends, :user_id).each do |hash| %>
    <% friend = hash[:eager_loaded_user] %>
    <dt class="friend-date"><%= hash[:created_at] %></dt><dd class="friend-friend"><a href="/profile/<%= friend[:account_name] %>"><%= friend[:nick_name] %></a></dd>
    <% end %>
  </dl>
</div>
